{
  "name": "@tappleby/react-typeahead-component",
  "description": "Typeahead, written using the React.js library.",
  "author": "Ezequiel Rodriguez <ezequiel@yahoo.com>",
  "version": "0.10.3",
  "main": "./lib/index.js",
  "license": "MIT",
  "bugs": "https://github.com/ezequiel/react-typeahead-component/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ezequiel/react-typeahead-component.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typeahead",
    "autocomplete",
    "react-component",
    "component"
  ],
  "files": [
    "dist",
    "src",
    "lib"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run dist:dev && npm run dist:min",
    "build:lib": "babel src --out-dir lib",
    "clean": "rimraf lib dist",
    "dist:dev": "webpack src/index.js dist/react-typeahead-component.dev.js --config webpack.config.development.js",
    "dist:min": "webpack src/index.js dist/react-typeahead-component.min.js --config webpack.config.production.js",
    "prepublish": "npm run clean && npm run build",
    "test": "karma start",
    "test:dev": "karma start --no-single-run --browsers ChromeHeadless"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0-rc || ^16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "insert-css": "^0.2.0",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^2.0.9",
    "lolex": "^1.4.0",
    "mocha": "^2.2.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.14.0",
    "webpack": "^3.10.0"
  }
}
